{
  "manifest_version": 3,
  "name": "Speech to Text Extension",
  "version": "1.0.0",
  "description": "Speech recognition extension",
  "permissions": ["storage", "activeTab", "scripting", "audioCapture"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
